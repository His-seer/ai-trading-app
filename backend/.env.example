# Environment Variables
# Copy this file to .env and fill in your API keys

# Google Gemini API Key
# Get yours at: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Finnhub API Key (Free)
# Get yours at: https://finnhub.io/register
FINNHUB_API_KEY=your_finnhub_api_key_here

# Twelve Data API Key (Free)
# Get yours at: https://twelvedata.com/
TWELVEDATA_API_KEY=your_twelvedata_api_key_here

# Server Configuration
PORT=3001
NODE_ENV=development

# Frontend URL (for CORS - set this in Railway to your frontend URL)
FRONTEND_URL=

# Trading Configuration
INITIAL_BALANCE=10000
MAX_RISK_PER_TRADE=0.02
MAX_TRADES_PER_DAY=3
AUTONOMY_INTERVAL_MINUTES=15
